<?xml version="1.0" encoding="UTF-8"?>

<widget xmlns     = "http://www.w3.org/ns/widgets"
        xmlns:gap = "http://phonegap.com/ns/1.0"
        id        = "com.deependhulla.com.memberverificationsquash"
        versionCode   = "11"
        version   = "1.1"
>

    <name>Squash Member Verification</name>

    <description>
Member Verification using Mfare RFID card via PhoneGAP and NFC on Android Phone/Tablet.
</description>

    <author href="http://www.deepen.in" email="callme@deepen.in">
        Deepen Dhulla
    </author>
 <access origin="*" />

    <preference name="permissions" value="none"/>
    <preference name="phonegap-version" value="3.7.0" /> 
    <preference name="fullscreen" value="true" />
    <preference name="webviewbounce" value="true" />
    <preference name="orientation" value="portrait" />
<icon src="res/icon/android/man129.png" />

    <content src="index.html" />

<gap:plugin name="com.chariotsolutions.nfc.plugin" version="0.5.1" />    
<gap:plugin name="org.apache.cordova.dialogs" version="0.2.10" />
<gap:plugin name="com.chariotsolutions.nfc.plugin" version="0.5.1" />
<gap:plugin name="it.adacto.cordova.device" version="0.1" />

</widget>
